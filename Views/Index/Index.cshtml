@model LidyCms.Model.All
@using LidyCms.Web.UI
@{
    BasePage bp = new BasePage();
    ViewBag.Title = bp.SiteConfig.WebName;
    ViewBag.Keywords = bp.SiteConfig.WebKeywords;
    ViewBag.Description = bp.SiteConfig.WebDescription;
}
@section AddToHead{
    <link href="@Url.Content("/Content/Css/camera.css")" rel="stylesheet/less" type="text/css" />
}
<div class="container">
<div class="camera_wrap camera_azure_skin" id="camera_wrap_1">
            <div data-thumb="/content/images/focus1.jpg" data-src="/content/images/focus1.jpg">
                <div class="camera_caption fadeFromBottom">
                   星恒星产品展示
                </div>
            </div>
            <div data-thumb="/content/images/focus2.jpg" data-src="/content/images/focus2.jpg">
                <div class="camera_caption fadeFromBottom">
                    星恒星产品展示
                </div>
            </div>
            <div data-thumb="/content/images/focus3.jpg" data-src="/content/images/focus3.jpg">
                <div class="camera_caption fadeFromBottom">
                    星恒星产品展示
                </div>
            </div>
            <div data-thumb="/content/images/focus4.jpg" data-src="/content/images/focus4.jpg">
                <div class="camera_caption fadeFromBottom">
                     星恒星产品展示
                </div>
            </div>
        </div>
</div>
<script src="@Url.Content("~/Content/Scripts/camera.min.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Content/Scripts/jquery.mobile.customized.min.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Content/Scripts/jquery.easing.1.3.js")" type="text/javascript"></script>

<script type="text/javascript">
    jQuery(function () {
        jQuery('#camera_wrap_1').camera({
            playPause:false,
            thumbnails:false,
            pagination: false,
            fx:"simpleFade"
        });

    });
</script>

