  <rewrite name="picture_show"
        path="/picture/show/{0}.aspx"
        pattern = "/picture/show/(\d+).aspx$"
        page="/picture_show.aspx"
        querystring="id=$1" />
  
  <rewrite name="download"
        path="/download.aspx"
        pattern = "/download.aspx$"
        page="/download.aspx"
        querystring="" />
  <rewrite name="download_list1"
        path="/download/{0}.aspx"
        pattern = "/download/(\d+)*.aspx$"
        page="/download.aspx"
        querystring="classId=$1" />
  <rewrite name="download_list2"
        path="/download/{0}/{1}.aspx"
        pattern = "/download/(\d+)*/(\w+).aspx$"
        page="/download.aspx"
        querystring="classId=$1^page=$2" />
  <rewrite name="download_show"
        path="/download/show/{0}.aspx"
        pattern = "/download/show/(\d+).aspx$"
        page="/download_show.aspx"
        querystring="id=$1" />
  
  <rewrite name="content"
        path="/content/{0}.aspx"
