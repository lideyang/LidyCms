<?xml version="1.0" encoding="utf-8" ?> 
<urls>
  <rewrite name="index"
        path="/index.aspx"
        pattern = "/index.aspx$"
        page="/index.aspx"
        querystring="" />

  <rewrite name="News"
        path="/News.aspx"
        pattern = "/News.aspx$"
        page="/News.aspx"
        querystring="" />
  <rewrite name="News_list1"
        path="/News/{0}.aspx"
        pattern = "/News/(\d+)*.aspx$"
        page="/News.aspx"
        querystring="classId=$1" />
  <rewrite name="News_list2"
        path="/News/{0}/{1}.aspx"
        pattern = "/News/(\d+)*/(\w+).aspx$"
        page="/News.aspx"
        querystring="classId=$1^page=$2" />
  <rewrite name="News_show"
        path="/News/show/{0}.aspx"
        pattern = "/News/show/(\d+).aspx$"
        page="/News_show.aspx"
        querystring="id=$1" />
  
  <rewrite name="picture"
        path="/picture.aspx"
        pattern = "/picture.aspx$"
        page="/picture.aspx"
        querystring="" />
  <rewrite name="picture_list1"
